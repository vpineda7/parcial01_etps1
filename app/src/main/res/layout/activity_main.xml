<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/edtPrecioHora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintTop_toBottomOf="@+id/edtHorasTrabajadas"
        tools:layout_editor_absoluteX="48dp" />

    <EditText
        android:id="@+id/edtHorasTrabajadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintTop_toBottomOf="@+id/edtNombre"
        tools:layout_editor_absoluteX="48dp" />

    <EditText
        android:id="@+id/edtNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="48dp" />

    <Button
        android:id="@+id/btnCalcular"
        android:layout_width="303dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:text="Calcular"
        app:layout_constraintTop_toBottomOf="@+id/edtPrecioHora"
        tools:layout_editor_absoluteX="54dp" />

    <TextView
        android:id="@+id/tvNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/btnCalcular"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvHorasTrabajadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvNombre"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvPago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvHorasTrabajadas"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvNormales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/tvPago"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvDobles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvNormales"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvTriples"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvDobles"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/tvPagoNeto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvTriples"
        tools:layout_editor_absoluteX="48dp" />

    <Button
        android:id="@+id/btnNuevo"
        android:layout_width="261dp"
        android:layout_height="53dp"
        android:layout_marginBottom="44dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="64dp" />
</androidx.constraintlayout.widget.ConstraintLayout>